{
  "name": "axelote-fe-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@axelote/js": "^0.1.8-alpha",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.14",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "concurrently": "^7.6.0",
    "npm-watch": "^0.11.0",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primereact": "^9.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "watch": {
    "dev-axelote": "src/**/*.tsx"
  },
  "scripts": {
    "dev": "concurrently \"npm start\" \"npm-watch dev-axelote\"",
    "start": "react-scripts start",
    "dev-axelote": "axelote hash -f src/**/*.tsx --noWrite",
    "build": "react-scripts build && axelote hash -f build/**/*.tsx",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
